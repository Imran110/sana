<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<ImageView android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:src="@drawable/logo" />
	<!-- <Button android:id="@+id/moca_main_record"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:text="Lookup Patient Record" /> -->
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:gravity="center">
		<Button android:id="@+id/moca_main_procedure"
			android:layout_width="120px"
			android:layout_height="wrap_content" android:text="Perform Procedure" />
		<Button android:id="@+id/moca_main_transfers"
			android:layout_width="120px"
			android:layout_height="wrap_content" android:text="View Saved Procedures"/>
		<Button android:id="@+id/moca_main_notifications"
			android:layout_width="120px"
			android:layout_height="wrap_content"
			android:text="View Notifications" />
	</LinearLayout>
</LinearLayout>
